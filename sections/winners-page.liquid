        let minutesVal = parseInt(minutes.textContent);
        minutesVal = minutesVal > 0 ? minutesVal - 1 : 59;
        minutes.textContent = minutesVal < 10 ? `0${minutesVal}` : minutesVal;
        
        // If minutes roll over, update hours
