<style>
  :root {
    --primary-red: #e5172c;
    --dark-red: #b5121e;
    --black: #121212;
    --light-gray: #f2f2f2;
    --white: #ffffff;
  }

  .page-header {
    background: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7))
    , url('https://cdn.shopify.com/s/files/1/0667/8036/4971/files/photo_2025-04-22_14-49-29.jpg?v=1745318990') center/cover no-repeat;
    padding: 100px 0;
    text-align: center;
    position: relative;
  }

  .page-header::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100px;
    background: linear-gradient(to top, var(--black), transparent);
  }

  .header-content {
    position: relative;
    z-index: 2;
  }

  .page-title {
    font-size: 3rem;
    font-weight: 800;
    margin-bottom: 20px;
    text-transform: uppercase;
    color: var(--white);
    position: relative;
    display: inline-block;
    padding-bottom: 15px;
  }

  .page-title::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 80px;
    height: 4px;
    background-color: var(--primary-red);
  }

  .highlight {
    color: var(--primary-red);
  }

  .section {
    padding: 80px 0;
  }

  .section-alt {
    background-color: #0a0a0a;
  }

  .section-title {
    font-size: 2.5rem;
    text-align: center;
    margin-bottom: 60px;
    text-transform: uppercase;
    color: var(--white);
    position: relative;
  }

  .section-title::after {
    content: '';
    position: absolute;
    bottom: -15px;
    left: 50%;
    transform: translateX(-50%);
    width: 100px;
    height: 4px;
    background-color: var(--primary-red);
  }

  .cta-button {
    display: inline-block;
    background-color: var(--primary-red);
    color: var(--white);
    font-size: 1.2rem;
    font-weight: 600;
    padding: 15px 40px;
    border-radius: 50px;
    text-decoration: none;
    text-transform: uppercase;
    letter-spacing: 1px;
    transition: all 0.3s ease;
    border: none;
    cursor: pointer;
    box-shadow: 0 5px 15px rgba(229, 23, 44, 0.4);
  }

  .cta-button:hover {
    background-color: var(--dark-red);
    transform: translateY(-3px);
    box-shadow: 0 8px 20px rgba(229, 23, 44, 0.6);
  }

  /* Winners Page Specific Styles */
  .winners-intro {
    text-align: center;
    max-width: 800px;
    margin: 0 auto
  }

  .winners-intro p {
    font-size: 14px;
    margin-bottom: 30px;
    padding: 0;
    margin: 0;
  }

  .winners-filter {
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    gap: 15px;
    margin-bottom: 50px;
  }

  .filter-btn {
    background-color: rgba(255, 255, 255, 0.1);
    border: 2px solid transparent;
    color: var(--white);
    padding: 10px 25px;
    border-radius: 30px;
    cursor: pointer;
    font-weight: 600;
    transition: all 0.3s ease;
  }

  .filter-btn:hover,
  .filter-btn.active {
    border-color: var(--primary-red);
    background-color: rgba(229, 23, 44, 0.1);
  }

  .filter-btn.active {
    color: var(--primary-red);
  }

  .winners-grid {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(350px, 1fr));
    gap: 40px;
    margin-bottom: 60px;
  }

  .winner-card {
    background-color: rgba(255, 255, 255, 0.05);
    border-radius: 15px;
    overflow: hidden;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);
    transition: transform 0.3s ease;
    border-bottom: 3px solid var(--primary-red);
  }

  .winner-card:hover {
    transform: translateY(-10px);
  }

  .winner-img-container {
    position: relative;
    height: 250px;
    overflow: hidden;
  }

  .winner-img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.5s ease;
  }

  .winner-card:hover .winner-img {
    transform: scale(1.05);
  }

  .winner-prize-badge {
    position: absolute;
    top: 20px;
    right: 20px;
    background-color: var(--primary-red);
    color: var(--white);
    padding: 10px 15px;
    border-radius: 30px;
    font-weight: 700;
    font-size: 0.9rem;
  }

  .winner-content {
    padding: 25px;
  }

  .winner-name {
    font-size: 1.5rem;
    margin-bottom: 10px;
    color: var(--white);
  }

  .winner-date {
    color: var(--primary-red);
    font-weight: 600;
    margin-bottom: 15px;
    font-size: 1rem;
  }

  .winner-story {
    margin-bottom: 20px;
  }

  .quote {
    font-size: 16px;
    font-style: italic;
    position: relative;
    padding-left: 20px;
    border-left: 3px solid var(--primary-red);
    margin: 20px 0;
  }

  .next-giveaway {
    background: linear-gradient(rgba(0, 0, 0, 0.8), rgba(0, 0, 0, 0.8))
    , url('/api/placeholder/1200/600') center/cover no-repeat;
    border-radius: 15px;
    padding: 0;
    text-align: center;
    position: relative;
  }

  .next-giveaway h3 {
    font-size: 2.2rem;
    margin-bottom: 20px;
    color: var(--white);
  }

  .next-giveaway p {
    font-size: 1.2rem;
    margin-bottom: 30px;
    max-width: 800px;
    margin-left: auto;
    margin-right: auto;
  }

  .timer-container {
    display: flex;
    justify-content: center;
    gap: 20px;
    margin: 40px 0;
  }

  .timer-item {
    background-color: rgba(0, 0, 0, 0.7);
    padding: 20px;
    border-radius: 10px;
    min-width: 100px;
    border: 1px solid var(--primary-red);
  }

  .timer-number {
    font-size: 2.5rem;
    font-weight: 700;
    color: var(--white);
  }

  .timer-label {
    font-size: 1rem;
    color: var(--primary-red);
  }

  .testimonial-section {
    text-align: center;
  }

  .testimonial-slider {
    max-width: 800px;
    margin: 0 auto;
    padding: 40px 20px;
    background-color: rgba(255, 255, 255, 0.05);
    border-radius: 15px;
    position: relative;
  }

  .testimonial-slide {
    padding: 20px;
  }

  .testimonial-quote {
    font-size: 1.3rem;
    font-style: italic;
    margin-bottom: 30px;
    position: relative;
  }

  .testimonial-quote::before,
  .testimonial-quote::after {
    content: '"';
    color: var(--primary-red);
    font-size: 3rem;
    line-height: 0;
    position: relative;
  }

  .testimonial-quote::before {
    top: 15px;
    margin-right: 5px;
  }

  .testimonial-quote::after {
    bottom: -5px;
    margin-left: 5px;
  }

  .testimonial-author {
    font-weight: 700;
    color: var(--white);
    font-size: 1.1rem;
  }

  .testimonial-prize {
    color: var(--primary-red);
    font-weight: 500;
  }

  .winner-spotlight {
    margin-top: 100px;
    background-color: rgba(229, 23, 44, 0.05);
    border-radius: 15px;
    padding: 40px;
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 40px;
    align-items: center;
  }

  .spotlight-img {
    width: 100%;
    border-radius: 10px;
    border: 3px solid var(--primary-red);
  }

  .spotlight-content h3 {
    font-size: 2rem;
    margin-bottom: 15px;
    color: var(--white);
  }

  .spotlight-prize {
    color: var(--primary-red);
    font-weight: 700;
    font-size: 1.2rem;
    margin-bottom: 20px;
  }

  .spotlight-story {
    font-size: 14px;
    margin-bottom: 30px;
  }

  .participation-steps {
    margin-top: 80px;
  }

  .steps-container {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 30px;
  }

  .step-card {
    background-color: rgba(255, 255, 255, 0.05);
    padding: 30px;
    border-radius: 15px;
    text-align: center;
    border-bottom: 3px solid var(--primary-red);
  }

  .step-number {
    width: 50px;
    height: 50px;
    background-color: var(--primary-red);
    color: var(--white);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 1.5rem;
    font-weight: 700;
    margin: 0 auto 20px;
  }

  .step-title {
    font-size: 1.5rem;
    margin-bottom: 15px;
    color: var(--white);
  }

  .step-text {
    color: var(--light-gray);
  }

  footer {
    background-color: var(--black);
    padding: 40px 0;
    color: var(--light-gray);
  }

  .footer-content {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    gap: 40px;
  }

  .footer-column {
    flex: 1;
    min-width: 250px;
  }

  .footer-title {
    font-size: 1.2rem;
    margin-bottom: 20px;
    color: var(--white);
  }

  .footer-links {
    list-style: none;
  }

  .footer-links li {
    margin-bottom: 10px;
  }

  .footer-links a {
    color: var(--light-gray);
    text-decoration: none;
    transition: color 0.3s ease;
  }

  .footer-links a:hover {
    color: var(--primary-red);
  }

  .social-icons {
    display: flex;
    gap: 15px;
    margin-top: 20px;
  }

  .social-icon {
    width: 40px;
    height: 40px;
    background-color: rgba(255, 255, 255, 0.1);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: background-color 0.3s ease;
    color: var(--white);
    text-decoration: none;
  }

  .social-icon:hover {
    background-color: var(--primary-red);
  }

  .copyright {
    text-align: center;
    padding-top: 40px;
    color: rgba(255, 255, 255, 0.5);
    font-size: 0.9rem;
  }

  .mobile-menu-btn {
    display: none;
    background: none;
    border: none;
    color: var(--white);
    font-size: 1.5rem;
    cursor: pointer;
  }

  .floating-cta {
    position: fixed;
    bottom: 30px;
    right: 30px;
    z-index: 100;
  }

  @media (max-width: 992px) {
    .winners-grid {
      grid-template-columns: repeat(2, 1fr);
    }

    .winner-spotlight,
    .steps-container {
      grid-template-columns: 1fr 1fr;
    }

    .timer-container {
      flex-wrap: wrap;
    }
  }

  @media (max-width: 768px) {
    .winners-grid,
    .winner-spotlight,
    .steps-container {
      grid-template-columns: 1fr;
    }

    .mobile-menu-btn {
      display: block;
    }

    .nav-list {
      position: absolute;
      top: 80px;
      left: 0;
      width: 100%;
      background-color: var(--black);
      flex-direction: column;
      align-items: center;
      padding: 20px 0;
      gap: 20px;
      transform: translateY(-150%);
      transition: transform 0.3s ease;
      z-index: 99;
      box-shadow: 0 10px 20px rgba(0, 0, 0, 0.3);
    }

    .nav-list.active {
      transform: translateY(0);
    }

    .nav-list li {
      margin: 0;
    }

    .page-title {
      font-size: 2.2rem;
    }

    .timer-item {
      min-width: 80px;
    }
  }
</style>

