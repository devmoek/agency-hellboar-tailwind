          hoursVal = hoursVal > 0 ? hoursVal - 1 : 23;
          hours.textContent = hoursVal < 10 ? `0${hoursVal}` : hoursVal;
          
          // If hours roll over, update days
          if (hoursVal === 23) {
            let daysVal = parseInt(days.textContent);
            daysVal = daysVal > 0 ? daysVal - 1 : 0;
            days.textContent = daysVal;
          }
        }
      }
    }
    
