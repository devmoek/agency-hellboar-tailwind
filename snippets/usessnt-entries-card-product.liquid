{%- liquid
  assign price = product.price
  assign price_in_dollars = price | divided_by: 100.0
-%}

<!-- Debug output -->
<div>
  Debug:
  Raw price: {{ price }}
  Price in dollars: {{ price_in_dollars }}
  Product object: {{ product | json }}
</div>

{% assign multiplier = 5 %}
{% if product.metafields.entries_multiplier.value != blank %}
  {% assign multiplier = product.metafields.entries_multiplier.value | plus: 0 %}
{% endif %}

{% assign entries = price_in_dollars | times: multiplier | round %}

<div class="product-ticket-entries entry-bonus" style="float:unset;">
  <span class="entries">
    <span class="product-item-icon">ðŸŽŸ</span>
    <span class="entries_cl">{{ entries }} Entries</span>
  </span>
</div>