<div class="product-ticket-entries entry-bonus" style="float:unset;">
    {% assign multiplier = 5 %}

    {% assign multiplier = card_product.metafields.entries_multiplier %}

    {% assign entries = card_product.price | divided_by: 100 | times: multiplier %}

    <span class="entries">
        <span class="product-item-icon" style="">🎟</span>
        <span class="entries_cl">{{ entries }} Entries</span>
    </span>
</div>
